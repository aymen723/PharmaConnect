{
  "expo": {
    "name": "Pharmaconnect",
    "slug": "pharmaconnect",
    "scheme": "pharmaconnect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Images/Pharma_Connect.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Images/Pharma_Connect.png",
      "resizeMode": "contain",
      "backgroundColor": "#15BD92"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.Pharmaconnect.Mobile"
    },
    "android": {
      "package": "com.Pharmaconnect.Mobile",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDY2NX8jLD_wH7JjxFBJH9XUvE0nbsTEb8"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5aef722b-c9b4-4409-bf4b-221630d1c17d"
      }
    },
    "owner": "aymen723",
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Montserrat.ttf",
            "./assets/fonts/Montserrat-Italic.ttf"
          ]
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Pharmaconnect to use your location."
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.AIzaSyCScqwYVRKJED4om7cmwicHTbnFc6PtVME"
        }
      ]
    ]
  }
}